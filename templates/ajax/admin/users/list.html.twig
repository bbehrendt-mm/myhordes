{% block ajax %}
    {% for user in users %}
        <div class="users-list-entry" x-user-id="{{ user.id }}" x-user-name="{{ user.name }}" x-user-username="{{ user.username }}" x-admin-url="{{ url('admin_users_citizen_view', {id: user.id}) }}">
            <div {% if not nohref %}x-ajax-href="{{ url('admin_users_citizen_view', {id: user.id}) }}" x-ajax-target="#content"{% endif %}>
                {% include "ajax/soul/playeravatar.html.twig" with {user: user, small: true, attributes: {style: 'margin-right: 0'}} only %}
                {{ user.name }}
            </div>
        </div>
    {% endfor %}
{% endblock %}