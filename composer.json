        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "adbario/php-dot-notation": "^3.3",
        "bentools/webpush-bundle": "^0.11.0",
        "brainbox-et/etwin-api-client": "@dev",
        "clue/graph": "0.9.3",
        "composer/semver": "^3.4",
        "doctrine/annotations": "2.0.1",
        "doctrine/doctrine-bundle": "2.11.1",
        "doctrine/doctrine-fixtures-bundle": "3.5.1",
        "doctrine/doctrine-migrations-bundle": "3.3.0",
        "doctrine/migrations": "3.7.1",
        "doctrine/orm": "2.17.1",
        "graphp/algorithms": "0.8.2",
        "jawira/case-converter": "^3.5",
        "jdrieghe/array-helpers": "^0.2.0",
        "laminas/laminas-code": "4.13.0",
        "league/commonmark": "2.4.1",
        "myhordes/fixtures": "@dev",
        "myhordes/plugins": "@dev",
        "myhordes/prime": "^2.0.0",
        "nikic/php-parser": "^4.15",
        "nopjmp/discord-webhooks": "^0.3.1",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "phpstan/phpdoc-parser": "^1.13",
        "scienta/doctrine-json-functions": "^5.2",
        "sentry/sentry-symfony": "^4.12",
        "shivas/versioning-bundle": "dev-master",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "6.4.0",
        "symfony/console": "6.4.1",
        "symfony/crowdin-translation-provider": "6.4.0",
        "symfony/doctrine-messenger": "6.4.1",
        "symfony/dotenv": "6.4.0",
        "symfony/expression-language": "6.4.0",
        "symfony/flex": "v2.4.2",
        "symfony/form": "v6.4.1",
        "symfony/framework-bundle": "6.4.1",
        "symfony/http-client": "6.4.0",
        "symfony/intl": "6.4.0",
        "symfony/lock": "6.4.0",
        "symfony/mailer": "6.4.0",
        "symfony/messenger": "6.3.7",
        "symfony/monolog-bundle": "3.10.0",
        "symfony/notifier": "6.4.0",
        "symfony/process": "6.4.0",
        "symfony/property-access": "6.4.0",
        "symfony/property-info": "6.4.0",
        "symfony/rate-limiter": "6.4.0",
        "symfony/security-bundle": "6.4.0",
        "symfony/serializer": "6.4.1",
        "symfony/string": "6.4.0",
        "symfony/translation": "6.4.0",
        "symfony/twig-bundle": "6.4.0",
        "symfony/uid": "6.4.0",
        "symfony/validator": "6.4.0",
        "symfony/web-link": "6.4.0",
        "symfony/webpack-encore-bundle": "2.1.1",
        "symfony/yaml": "6.4.0",
        "twig/cache-extra": "^3.8",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "3.8.0",
        "twig/markdown-extra": "3.8.0",
        "twig/twig": "^2.12|^3.0",
        "wikimedia/composer-merge-plugin": "^2.1",
        "zenstruck/schedule-bundle": "^1.5"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "dama/doctrine-test-bundle": "7.2.1",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "10.4.2",
        "rector/rector": "0.18.10",
        "symfony/browser-kit": "6.4.0",
        "symfony/css-selector": "6.4.0",
        "symfony/debug-bundle": "6.4.0",
        "symfony/maker-bundle": "1.52.0",
        "symfony/phpunit-bridge": "6.4.0",
        "symfony/stopwatch": "6.4.0",
        "symfony/web-profiler-bundle": "6.4.0"
    },
    "suggest": {
        "ext-imagick": "*",
        "ext-pthreads": "*",
        "ext-ssh2": "*",
        "ext-apcu": "*"
    },
    "repositories": [
      {
        "type": "path",
        "url": "packages/myhordes-*"
      }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "wikimedia/composer-merge-plugin": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "0.1.*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        },
        "merge-plugin": {
          "include": [
            "composer.dist.json"
          ]
        }
    }
}
