{
    "type": "project",
    "license": "proprietary",
    "name": "etwin/myhordes",
    "description": "",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "brainbox-et/etwin-api-client": "0.1.1",
        "clue/graph": "0.9.3",
        "doctrine/annotations": "1.13.3",
        "doctrine/doctrine-bundle": "2.7.0",
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/migrations": "3.5.2",
        "doctrine/orm": "2.13.3",
        "graphp/algorithms": "0.8.2",
        "laminas/laminas-code": "4.7.0",
        "league/commonmark": "2.3.5",
        "myhordes/fixtures": "@dev",
        "myhordes/plugins": "@dev",
        "myhordes/prime": "@dev",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "phpstan/phpdoc-parser": "^1.13",
        "sensio/framework-extra-bundle": "6.2.8",
        "shivas/versioning-bundle": "dev-master",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "6.1.5",
        "symfony/console": "6.1.6",
        "symfony/crowdin-translation-provider": "6.1.4",
        "symfony/dotenv": "6.1.0",
        "symfony/expression-language": "6.1.6",
        "symfony/flex": "2.2.3",
        "symfony/form": "6.1.5",
        "symfony/framework-bundle": "6.1.6",
        "symfony/http-client": "6.1.6",
        "symfony/intl": "6.1.6",
        "symfony/lock": "6.1.3",
        "symfony/mailer": "6.1.5",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/notifier": "6.1.0",
        "symfony/process": "6.1.3",
        "symfony/property-access": "6.1.3",
        "symfony/property-info": "6.1.6",
        "symfony/rate-limiter": "6.1.3",
        "symfony/security-bundle": "6.1.3",
        "symfony/serializer": "6.1.6",
        "symfony/string": "6.1.6",
        "symfony/translation": "6.1.6",
        "symfony/twig-bundle": "6.1.1",
        "symfony/validator": "6.1.6",
        "symfony/web-link": "6.1.0",
        "symfony/webpack-encore-bundle": "1.16.0",
        "symfony/yaml": "6.1.6",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "3.4.2",
        "twig/markdown-extra": "3.4.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "7.1.1",
        "phpunit/phpunit": "9.5.26",
        "rector/rector": "0.14.6",
        "symfony/browser-kit": "6.1.3",
        "symfony/css-selector": "6.1.3",
        "symfony/debug-bundle": "6.1.3",
        "symfony/maker-bundle": "1.47.0",
        "symfony/phpunit-bridge": "6.1.6",
        "symfony/stopwatch": "6.1.5",
        "symfony/web-profiler-bundle": "6.1.6"
    },
    "suggest": {
        "ext-imagick": "*",
        "ext-pthreads": "*",
        "ext-ssh2": "*"
    },
    "repositories": [
      {
        "type": "path",
        "url": "packages/*"
      }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "0.1.*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
