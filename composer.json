    "name": "etwin/myhordes",
    "description": "",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
