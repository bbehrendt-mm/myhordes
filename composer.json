{
    "type": "project",
    "license": "proprietary",
    "name": "etwin/myhordes",
    "description": "",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "brainbox-et/etwin-api-client": "0.1.1",
        "clue/graph": "0.9.3",
        "doctrine/annotations": "1.13.2",
        "doctrine/doctrine-bundle": "2.6.2",
        "doctrine/doctrine-fixtures-bundle": "3.4.1",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/migrations": "3.5.0",
        "doctrine/orm": "2.11.2",
        "graphp/algorithms": "0.8.2",
        "laminas/laminas-code": "4.5.1",
        "myhordes/plugins": "@dev",
        "myhordes/fixtures": "@dev",
        "myhordes/prime": "@dev",
        "phpdocumentor/reflection-docblock": "5.3.0",
        "sensio/framework-extra-bundle": "6.2.6",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "v6.0.7",
        "symfony/console": "6.0.11",
        "symfony/crowdin-translation-provider": "v6.0.8",
        "symfony/dotenv": "6.0.5",
        "symfony/expression-language": "v6.0.11",
        "symfony/flex": "1.18.6",
        "symfony/form": "v6.0.11",
        "symfony/framework-bundle": "v6.0.11",
        "symfony/http-client": "v6.0.11",
        "symfony/intl": "v6.0.8",
        "symfony/lock": "v6.0.10",
        "symfony/mailer": "v6.0.11",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/notifier": "v6.0.8",
        "symfony/process": "v6.0.11",
        "symfony/property-access": "v6.0.11",
        "symfony/property-info": "v6.0.11",
        "symfony/rate-limiter": "v6.0.11",
        "symfony/security-bundle": "v6.0.11",
        "symfony/serializer": "v6.0.11",
        "symfony/string": "v6.0.11",
        "symfony/translation": "v6.0.11",
        "symfony/twig-bundle": "v6.0.8",
        "symfony/validator": "v6.0.11",
        "symfony/web-link": "6.0.3",
        "symfony/webpack-encore-bundle": "v1.14.1",
        "symfony/yaml": "v6.0.11",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "3.3.5",
        "twig/twig": "^2.12|^3.0",
        "shivas/versioning-bundle": "4.0.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "6.7.5",
        "phpunit/phpunit": "9.5.20",
        "rector/rector": "0.12.20",
        "symfony/browser-kit": "v6.0.11",
        "symfony/css-selector": "v6.0.11",
        "symfony/debug-bundle": "v6.0.11",
        "symfony/maker-bundle": "1.38.0",
        "symfony/phpunit-bridge": "5.4.7",
        "symfony/stopwatch": "v6.0.5",
        "symfony/web-profiler-bundle": "v6.0.10"
    },
    "suggest": {
        "ext-imagick": "*",
        "ext-pthreads": "*",
        "ext-ssh2": "*"
    },
    "repositories": [
      {
        "type": "vcs",
        "url":  "https://github.com/wvsgnovize01/versioning-bundle.git"
      },
      {
        "type": "path",
        "url": "packages/*"
      }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "0.1.*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
