    "whichbrowser/parser": "^2.1",
    "wikimedia/composer-merge-plugin": "^2.1",
    "zenstruck/schedule-bundle": "^1.5"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "dama/doctrine-test-bundle": "v8.0.2",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-symfony": "^1.3",
    "phpunit/phpunit": "10.5.17",
    "rector/rector": "1.0.3",
    "symfony/browser-kit": "7.1.1",
    "symfony/css-selector": "7.1.1",
    "symfony/debug-bundle": "7.1.1",
    "symfony/phpunit-bridge": "7.1.4",
    "symfony/stopwatch": "7.1.1",
    "symfony/web-profiler-bundle": "7.1.4",
    "symfony/maker-bundle": "1.57.0"
  },
  "suggest": {
    "ext-imagick": "*",
    "ext-pthreads": "*",
    "ext-ssh2": "*",
    "ext-apcu": "*",
    "ext-ftp": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/myhordes-*"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "wikimedia/composer-merge-plugin": true,
      "php-http/discovery": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "0.1.*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.1.*"
    },
    "merge-plugin": {
      "include": [
        "composer.dist.json"
      ]
    }
  }
}
